{
	"name": "agents",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "tsgo -p tsconfig.build.json && cp -r src/presets/definitions dist/presets/definitions",
		"dev": "tsgo -p tsconfig.build.json --watch --preserveWatchOutput",
		"test": "vitest --run"
	},
	"dependencies": {
		"agent": "*",
		"ai": "*",
		"yaml": "^2.8.2"
	},
	"devDependencies": {
		"@types/node": "^24.3.0",
		"typescript": "^5.7.3",
		"vitest": "^3.2.4"
	}
}
